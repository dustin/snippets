#!/bin/sh
# $Id: ten,v 1.4 1997/12/15 09:32:39 dustin Exp $

. /home/monitor/lib/shellconf

umask 22

# normal scan things
$HOMEBIN/runscans &

# Other info
$HOME/stats/doit &

# netmon 2
$HOMEBIN/netmon2
chmod 644 $HOME/lib/plotdata

# $HOMEBIN/geticsorder &

# State logs
$HOMEBIN/statelog &

# get process lists
# $HOMEBIN/getpslist arthur tessa ics.ic3.com &
# $HOMEBIN/getdisklog arthur tessa ics.ic3.com &

# goldengate
# $HOMEBIN/goldengate &

$HOMEBIN/checkwebserver $libdir/urllist &

$HOMEBIN/checkpsql &

wait
