DEFS=@DEFS@
CFLAGS=@CFLAGS@ $(DEFS)
LDFLAGS=@LDFLAGS@
POSTGRESLIB=@POSTGRESLIB@
RRDLIB=@RRDLIB@

all: tablecounter

tablecounter: tablecounter.o array.o
	$(CC) -o $@ tablecounter.o array.o $(LDFLAGS) $(RRDLIB) $(POSTGRESLIB)

clean:
	rm -f tablecounter tablecounter.o array.o
